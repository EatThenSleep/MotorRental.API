                                <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001a1.007 1.007 0 0 0-.084.094l3.85 3.85a1 1 0 0 0 1.415-1.415l-3.85-3.85a1.007 1.007 0 0 0-.094-.084zm-5.79 0a5.5 5.5 0 1 1 7.778-7.778 5.5 5.5 0 0 1-7.778 7.778z" />
                            </svg>
                        </button>
                        <input type="text" class="form-control" style="width: 200px" placeholder="Gõ nội dung tìm kiếm" />
                        <button type="button" class="btn btn-light">
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="icon bi bi-printer" viewBox="0 0 16 16" title="Print">
                                <path d="M2 7a2 2 0 0 1 2-2h8a2 2 0 0 1 2 2v4a2 2 0 0 1-2 2v3a1 1 0 0 1-1 1H5a1 1 0 0 1-1-1v-3a2 2 0 0 1-2-2V7zm2-1a1 1 0 0 0-1 1v4a1 1 0 0 0 1 1h8a1 1 0 0 0 1-1V7a1 1 0 0 0-1-1H4z" />
                                <path d="M5 1a2 2 0 0 0-2 2v3h10V3a2 2 0 0 0-2-2H5z" />
